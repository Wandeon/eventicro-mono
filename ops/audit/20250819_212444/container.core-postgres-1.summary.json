{
  "name": "core-postgres-1",
  "image": "postgres:16",
  "binds": [
    "core_pg-a5-data:/var/lib/postgresql/data:rw",
    "/srv/walg/bin/wal-g:/usr/local/bin/wal-g:ro"
  ],
  "env_keys": [
    "AWS_S3_FORCE_PATH_STYLE",
    "AWS_REGION",
    "WALG_S3_PREFIX",
    "AWS_ACCESS_KEY_ID",
    "POSTGRES_PASSWORD",
    "POSTGRES_USER",
    "AWS_ENDPOINT",
    "AWS_SECRET_ACCESS_KEY",
    "PATH",
    "GOSU_VERSION",
    "LANG",
    "PG_MAJOR",
    "PG_VERSION",
    "PGDATA"
  ]
}
